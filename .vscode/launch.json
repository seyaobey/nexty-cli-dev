{
        "version": "0.2.0",
        "configurations": [
            {
                "type": "node",
                "request": "launch",
                "name": "chrome",                
                "program": "${workspaceRoot}\\server\\server.js",
                "outFiles": [
                    "${workspaceRoot}/server/**/*.js"
                ]
            },
            {
                "type": "node",
                "request": "launch",
                "name": "prompt",  
                "console": "integratedTerminal",
                "protocol": "inspector",
                "program": "${workspaceRoot}\\server\\nexty-cli\\index.js",
                "outFiles": [
                    "${workspaceRoot}/server/**/*.js"
                ]
            }
        ]
    }